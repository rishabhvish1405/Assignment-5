}
Question 9
#include <stdio.h>

int main()
{
    int n;
    printf("Enter a number: ");
    scanf("%d",&n);
    for(int i=1; n>= i; i++)
    {
        printf("\n %d",i*i*i);
     
    }

    return 0;
}
Question 10
#include <stdio.h>

int main()
{
    int n;
    printf("Enter a number: ");
    scanf("%d",&n);
    for(int i=1; 10>= i; i++)
    {
        printf("\n %d",i*n);
     
    }

    return 0;
}

Question 7
#include <stdio.h>

int main()
{
    int n;
    printf("Enter a number: ");
    scanf("%d",&n);
    int a= n*2;
    for(int i=1; n>= i; i++)
    {
        printf("\n %d",a);
        a=a-2;
    }

    return 0;
}
